{% extends "base.html.j2" %}

{% block app_content %}
<h1>Login</h1>
<form action="" method="POST">
    {{ form.csrf_token }}
    <p>
        {{ form.username.label }}<br/>
        {{ form.username }}
    </p>
    <p>
        {{ form.password.label }}<br/>
        {{ form.password }}
    </p>
    <p>
        {{ form.submit }}
    </p>
</form>
{% endblock %}